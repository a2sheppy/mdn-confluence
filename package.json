{
  "name": "mdn-confluence",
  "version": "0.0.1",
  "description": "User support tool for maintaining MDN compatability data using Web API Confluence data.",
  "scripts": {
    "build": "node main/import.es6.js https://storage.googleapis.com/web-api-confluence-data-cache/latest/json/org.chromium.apis.web.Release.json https://storage.googleapis.com/web-api-confluence-data-cache/latest/json/org.chromium.apis.web.GridRow.json",
    "postinstall": "mkdir -p data/confluence data/issues data/mdn",
    "start": "python -m SimpleHTTPServer",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mark Dittmer",
  "dependencies": {
    "foam2": "git://github.com/foam-framework/foam2.git#mdn-confluence",
    "mdn-browser-compat-data": "git://github.com/mdn/browser-compat-data.git",
    "web-api-confluence-dashboard": "git://github.com/mdittmer/confluence.git#mdn"
  }
}
